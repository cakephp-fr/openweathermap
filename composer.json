{
    "name": "cakephp-fr/openweathermap",
    "description": "Openweathermap plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "openweathermap"
    ],
    "homepage": "https://github.com/cakephp-fr/openweathermap",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Openweathermap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Openweathermap\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
